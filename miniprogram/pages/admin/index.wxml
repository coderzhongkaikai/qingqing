<van-collapse accordion value="{{ activeName }}" bind:change="onCollapseChange">
  <van-collapse-item title="编辑舞种" name="1">
    <!-- <van-cell-group> -->
    <van-field value="{{ dance_item }}" label="添加舞种" placeholder="请输入新增舞种名" />
    <van-button size="small" round type="info" block>圆形按钮</van-button>
    <!-- </van-cell-group> -->

    <van-divider contentPosition="center" customStyle="color: #1989fa; border-color: #1989fa; font-size: 28rpx;">
      已添加舞种
    </van-divider>

    <view class="dance_item_box">
      <view class="dance_item">
        <van-tag wx:if="{{ show.primary }}" closeable size="medium" type="primary" id="primary" bind:close="onTagClose">
          网红舞
        </van-tag>
      </view>
      <view class="dance_item">
        <van-tag wx:if="{{ show.primary }}" closeable size="medium" type="success" id="success" bind:close="onTagClose">
          网红舞
        </van-tag>
      </view>


    </view>

  </van-collapse-item>
  <van-collapse-item title="添加老师" name="2">
    <!-- <van-cell-group inset> -->
      <van-cell title="单元格" value="内容" />
      <van-cell title="单元格" value="内容" label="描述信息" />
    <!-- </van-cell-group> -->
  </van-collapse-item>
  <van-collapse-item title="活动列表" name="3">
    代码是写出来给人看的，附带能在机器上运行
  </van-collapse-item>
   


 
</van-collapse>
